// 3ms
// for循环中把len改为nums.size()将是6ms

class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int j = 0;
        int len = nums.size();
        for (int i = 0; i < len; ++i) {
            if (nums[i] != val) 
                nums[j++] = nums[i];
        }
        return j;
    }
};